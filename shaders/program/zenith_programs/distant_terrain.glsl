    float iz = floor(p.z);
    float fz = fract(p.z);
    
    // Ø­Ø³Ø§Ø¨ Ø§Ù„Ø¥Ø²Ø§Ø­Ø§Øª Ø§Ù„Ù…Ø­Ø³Ù†Ø© / Calculate enhanced offsets
    vec2 a_off = vec2(23.0, 29.0) * (iz) / 128.0;
    vec2 b_off = vec2(23.0, 29.0) * (iz + 1.0) / 128.0;
    
    // Ø£Ø®Ø° Ø¹ÙŠÙ†Ø§Øª Ø§Ù„Ø¶ÙˆØ¶Ø§Ø¡ / Sample noise
    float a = texture2D(noisetex, p.xy + a_off).r;
    float b = texture2D(noisetex, p.xy + b_off).r;
    
    // Ù…Ø²Ø¬ Ù†Ø§Ø¹Ù… Ø¨ÙŠÙ† Ø§Ù„Ù…Ø³ØªÙˆÙŠØ§Øª / Smooth blending between levels
    return mix(a, b, fz);
}

